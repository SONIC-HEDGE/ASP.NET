@model Web_App.Models.ContactModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Dodaj Kontakt</h2>
<form asp-action="Add">
    <div>
        <label asp-for="FirstName">Imię</label>
        <input asp-for="FirstName"/>
        <span asp-validation-for="FirstName" class="text-danger"></span>
        
        <button type="submit" class="btn btn-outline-primary">Didaj</button>
    </div>


</form>
